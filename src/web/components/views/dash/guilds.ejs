<div class="home-hero padding">
    <h3>Managed guilds</h3>
    <section class="ui-wrapper">
        <% managed.forEach(guild => { %> 
            <a href="/dash/guild/<%= guild.id %>">
            <div class="dash-guild-thumb big">
                <img src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>?size=64">
                <%= guild.name %>
                <div style="flex: 1"></div>
                <div class="btn full sq accent">
                    <svg style="width:24px;height:24px" viewBox="0 0 24 24">
                        <path fill="currentColor" d="M8.59,16.58L13.17,12L8.59,7.41L10,6L16,12L10,18L8.59,16.58Z" />
                    </svg>
                </div>
            </div>
        </a>
        <% }) %>
    </section><br><br><br>

    <h3>Other guilds</h3>
    <section class="ui-wrapper"">
        <% unmanaged.forEach(guild => { %> 
            <a href="/dash/guild/<%= guild.id %>">
                <div class="dash-guild-thumb">
                    <img src="https://cdn.discordapp.com/icons/<%= guild.id %>/<%= guild.icon %>?size=64">
                    <%= guild.name %>
                    <div style="flex: 1"></div>
                    <div class="btn full sq accent">
                        <svg style="width:24px;height:24px" viewBox="0 0 24 24">
                            <path fill="currentColor" d="M19,13H13V19H11V13H5V11H11V5H13V11H19V13Z" />
                        </svg>
                    </div>
                </div>
            </a>
        <% }) %>
    </section>
</div>